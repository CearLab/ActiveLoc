<launch>

    <!-- anchors params -->
    <arg name="AX" default="5" />
    <arg name="AY" default="5" />
    <arg name="AZ" default="0.184" />      

    <!-- ANCHORS SERVER -->
    <node pkg="jackal_range" name="anchors_server_real" type="anchors_server.py">
        <param name="anchors_pos" type="string" value="+$(arg AX) +$(arg AY) +$(arg AZ) -$(arg AX) +$(arg AY) +$(arg AZ) -$(arg AX) -$(arg AY) +$(arg AZ) +$(arg AX) -$(arg AY) +$(arg AZ)" />
        <param name="topic" type="string" value="AnchorsMarkersReal" />
        <param name="params_name" type="string" value="AnchorsInfoReal" />        
    </node>  

</launch>