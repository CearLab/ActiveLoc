<launch>
  <!-- Jackal configuration to view. See jackal_description for details. -->
  <arg name="config" default="base"/>
  <arg name="world_name" default="$(find jackal_gazebo)/worlds/empty.world" />

  <!-- Launch Gazebo with the specified world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="debug" value="0" />
    <arg name="gui" value="false" />
    <arg name="use_sim_time" value="true" />
    <arg name="headless" value="false" />
    <arg name="world_name" value="$(arg world_name)" />    
  </include>

  
  <!-- BEGIN ROBOT 1 -->
  <group ns="UGV01">    
    <include file="$(find jackal_custom)/launch/spawners/robot.launch" >
      <arg name="init_pose" value="-x 2 -y 1 -z 0.5 -R 0 -P 0 -Y 0" />
      <arg name="robot_name"  value="Jack01" />
      <arg name="joystick" value="true" />
      <arg name="joy_dev" value="/dev/input/js0" />
      <arg name="enable_ekf" value="true" /> 
      <arg name="ns" value="UGV01/" />      
    </include>        
  </group>

  <!-- BEGIN ROBOT 2 -->
  <!-- <group ns="UGV02">    
    <include file="$(find jackal_custom)/launch/spawners/robot.launch" >
      <arg name="init_pose" value="-x 2 -y -1 -z 0.5 -R 0 -P 0 -Y 0" />
      <arg name="robot_name"  value="Jack02" />
      <arg name="joystick" value="true" />
      <arg name="joy_dev" value="/dev/input/js0" />
      <arg name="enable_ekf" value="true" /> 
      <arg name="ns" value="UGV02/" />      
    </include>        
  </group> -->
  
  <!-- <node name="rviz" pkg="rviz" type="rviz" args="-d $(find jackal_viz)/rviz/model.rviz" /> -->
  <param name="use_gui" value="true"/>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find jackal_custom)/rviz/default.rviz" />
</launch>
